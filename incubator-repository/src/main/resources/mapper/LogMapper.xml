<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liuapi.incubator.repository.transaction.mapper.LogMapper">
    <insert id="addLog" parameterType="com.liuapi.incubator.repository.transaction.model.Log">
        insert into log (id,uid,`value`,created_at) values (#{id},#{uid},#{value},now());
    </insert>
</mapper>