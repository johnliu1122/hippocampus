<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.liuapi.incubator.repository.transaction.mapper.UserOpLogMapper">
    <insert id="addLog" parameterType="com.liuapi.incubator.repository.transaction.model.UserOpLog">
        insert into user_op_log (id,uid,op,created_at) values (#{id},#{uid},#{op},now());
    </insert>
</mapper>